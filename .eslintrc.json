{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-param-reassign": [2, { "props": false }],
    "no-use-before-define": ["error", { "functions": true, "classes": true }]
  }
}
